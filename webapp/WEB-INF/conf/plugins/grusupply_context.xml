<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" >
    
    <!-- REST -->
    <bean id="grusupply.grurestelasticsearch" class="fr.paris.lutece.plugins.grusupply.rest.GRURestElasticSearch"/>
    <bean id="grusupply.grurestdatabase" class="fr.paris.lutece.plugins.grusupply.rest.GRURestDataBase"/>
       
    
    <!-- SERVICE -->
    <bean id="grusupply.gruService" class="fr.paris.lutece.plugins.grusupply.service.GRUService"/>
    <bean class="fr.paris.lutece.plugins.grusupply.service.PluginRegistrationBeanPostProcessor">
		<constructor-arg ref="grusupply.gruService"/>
	</bean>	
    
</beans>
